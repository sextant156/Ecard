{
  "name": "@tarojs/plugin-platform-h5",
  "version": "3.6.8",
  "description": "Web 端平台插件",
  "author": "ZakaryCode",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "taro"
  ],
  "files": [
    "build",
    "dist",
    "index.js",
    "types"
  ],
  "sideEffects": [
    "./dist/runtime/index.js",
    "*.scss",
    "*.css"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "lodash-es": "4.17.21",
    "resolve": "^1.22.0",
    "@tarojs/service": "3.6.8",
    "@tarojs/components": "3.6.8",
    "@tarojs/components-react": "3.6.8",
    "@tarojs/router": "3.6.8",
    "@tarojs/shared": "3.6.8",
    "@tarojs/taro-h5": "3.6.8",
    "babel-plugin-transform-taroapi": "3.6.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "rollup": "^3.8.1",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-ts": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.4",
    "@tarojs/taro": "3.6.8"
  },
  "scripts": {
    "prebuild": "run-s clean generate:desc",
    "build": "pnpm run rollup --environment NODE_ENV:production",
    "clean": "rimraf dist",
    "dev": "pnpm run rollup --environment NODE_ENV:development -w",
    "generate:desc": "pnpm run tsx --files ./build/definition-json",
    "rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "tsx": "ts-node --skipIgnore"
  }
}